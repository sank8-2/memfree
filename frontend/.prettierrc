{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "singleAttributePerLine": false,
    "arrowParens": "always",
    "quoteProps": "consistent",
    "printWidth": 160,
    "plugins": ["prettier-plugin-tailwindcss"],
    "endOfLine": "lf"
}